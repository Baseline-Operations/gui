{
  "name": "my-repo",
  "library": false,
  "commands": {
    "test": "npm test",
    "lint": "npm run lint",
    "start": "npm start"
  },
  "watch": {
    "patterns": ["src/**/*.ts"],
    "ignore": ["node_modules/**", "dist/**"]
  },
  "startInDocker": false,
  "dockerImage": "node:20",
  "requiredPlugins": {
    "my-language-plugin": {
      "source": "npm",
      "version": "^1.0.0",
      "url": "baseline-plugin-typescript"
    },
    "my-custom-plugin": {
      "source": "git",
      "url": "https://github.com/user/baseline-custom-plugin.git",
      "version": "v1.2.0"
    }
  }
}

